#include <iostream>
#include <cmath>
using namespace std;
int main()
{
setlocale(LC_ALL, "ru");
double  h = 0.5,z0=0,z1=-1.02;
int n = 30, m = 10, x = 0;
double* z = new double[n];
z[0] = 0;
z[1] = -1.02;
for (int i = 2;i <= n;i++)
{
	z[i]= (1 - z[i-1]) / (1 + z[i-2] * z[i-2]) * sin(z[i-1] * z[i-1] + z[i-1] * z[i-2] + 2);
}
for (int j = 1;j <= m;j++)
{
	for (int i = 0;i <= n;i++)
	{
		if ((z[i] >= ((j - 1) * h)) && (z[i] < ( j * h)))
		{
			x++;
		}
	}
	cout << " " << x << " ";
	x = 0;
}
	return 0;
	system("pause");
}
