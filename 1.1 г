#include <iostream>
#include <cmath>
using namespace std;
int main()
{
double x, y, h = 0.05, b = -1, min = 5;
int n = 9;
double a[] = { -0.04,2,1.96 };
for (int i = 0; i < 3; i++)
{
for (int j = 1; j <= n; j++)
{
x = b + j * h;
if (y > min) {
y = (log(1 + 2 * a[i] + x * x)) + 1;
cout « y « " ";
}
if (y < min) { y = min; }
y = (log(1 + a[i] * a[i] + x * x)) - 1;
cout « y « " ";
}
}
return 0;
system("pause");
}
