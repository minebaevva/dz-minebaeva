#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    double x[3];
     x[1] = -0.1 ;
     x[2] = -10.5;
     x[3] = 5.8;
    int n=20;
    for (int i = 1; i <= 3; i++) {
        double y = 3.5*(sin(x[i]))*(sin(x[i]))-2.7*(sin(x[i])) + 1;
        double u = 1;
        for (int j = 1; j <= 20; j++) {
            u = 2 / (3 * u) + y / (3 * u * u);
        }
        cout << u << " ";
    }
    system("pause");
    return 0;
}
