#include <iostream> 
using namespace std; 
const int n=10; 
  int main(){ 
int a[n]; 
int i,j,m,c,k; 
cin>>a[i];
m=a[1];
c=1; k=0; 
    for(i=1;i<=n;i++) {
      for(j=1;j<=n;j++) 
if(a[i]==a[j])
  k++; 
if(((c == k) && (m > a[i])) || (c < k)){
m=a[i];
c=k;
  } 
k=0; 
  }
cout«"m="«m«"
count="«c«endl;
return 0;
}
